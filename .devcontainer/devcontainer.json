// https://github.blog/changelog/2022-10-26-codespaces-customizable-initial-experience
{
	"onCreateCommand": "git submodule update --init && bundle install",
	"postAttachCommand": "bundle exec jekyll serve --incremental",
	"portsAttributes": {
		"4000": {
			"label": "jekyll",
			"onAutoForward": "openPreview"
		}
	}
}
