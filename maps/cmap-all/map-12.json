{
  "#": "cmap-js example configuration file (json format)",
  "header" : ": Soybean QTL features",
  "attribution" : "data attribution: soybase, LIS",
  "initialView" : [ {
    "source" : "gm_composite",
    "map" : "H",
    "tracks" : [
      {
        "type" : "qtl",
        "title": "QTL",
        "type_acc": "QTL",
        "filters":["QTL"],
        "default_rank":2,
        "drawing_lane":2,
        "drawing_priority":1,
        "fillColor": ["black"],
        "shape": ["dumbbell"],
        "position":-1
      }
      ]
    },
    {
      "source" : "gm_gwas",
      "map" : "H"
    }
  ],
  "sources" : [ {
      "id" : "gm_gwas",
      "method": "GET",
      "url": "https://raw.githubusercontent.com/LegumeFederation/cmap-js/soy-test/data/Soybean-GmGWAS.cmap",
      "filters" : [],
      "config": {
        "method" : "GET",
        "url" : "https://raw.githubusercontent.com/LegumeFederation/cmap-js/soy-test/config/gm_gwas.json"
      },
      "linkouts" : [
        {
            "featuretype" : "gene",
            "url" : "https://legumeinfo.org/gene_links/${item.id}/json",
            "isLinkingService" : true
        }
      ]
    }, {
      "id" : "gm_composite",
      "method": "GET",
      "url" : "https://data.legumeinfo.org/annex/Glycine/max/maps/mixed.map.GmComposite2003/Soybean-GmComposite2003.cmap.gz?display=text",
      "filters" : [],
      "config": {
        "method" : "GET",
        "url" : "https://raw.githubusercontent.com/LegumeFederation/cmap-js/soy-test/config/gm_composite.json"
      },
      "linkouts" : [
        {
            "featuretypePattern" : "QTL",
            "url" : "https://soybase.org/sbt/search/search_results.php?category=QTLName&search_term=${item.id}",
            "text" : "View QTL info at SoyBase"
        },
        {
            "featuretype" : "SNP",
            "url" : "https://soybase.org/sbt/search/search_results.php?category=SNP&search_term=${item.id}",
            "text" : "View SNP info at SoyBase"
        },
        {
            "featuretypePattern" : "^(SSR|[AR]FLP|Gene|PCR)$",
            "url" : "https://soybase.org/sbt/search/search_results.php?category=LocusName&search_term=${item.id}",
            "text" : "View Marker info at SoyBase"
        }
      ]
    }
  ]
}
